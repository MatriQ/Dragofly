
<table>
<thead>
  <th>
    名称
  </th>
  <th>
    说明
  </th>
  <th>
    代码
  </th>
  <th>
    排序
  </th>
  <th>

  </th>
  <th>

  </th>
</thead>
<% funcs.forEach(function(func){ %>
<tr>
  <td>
    <%= func.name %>
  </td>
    <td>
      <%= func.desc %>
    </td>
      <td>
        <%= func.code %>
      </td>
          <td>
            <%= func.sort %>
          </td>
          <td>
            <a href="/func/edit/<%= func.id %>">编辑</a>
          </td>
          <td>
            <a href="/func/delete/<%= func.id %>" onclick="if(!confirm('删除后不可恢复,确定删除纪录吗?'))return false;">删除</a>
          </td>
</tr>
<%})%>
</table>
<br>
<a href="/func/new">创建</a>
<a href="/">首页</a>
