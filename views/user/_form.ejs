<div class="col-lg-12 col-sm-12 col-xs-12">
  <div class="row">
    <div class="widget">
      <div class="widget-header bordered-bottom">
        <span class="widget-caption">用户</span>
      </div>
      <div class="widget-body">
        <div>
          <input type="hidden" name="_csrf" value="<%= _csrf %>" />
          <div class="form-group">
            <label for="header">头像</label>
            <input name="header" type="text" class="form-control" id="tag" placeholder="TAG" value="<%= nav.tag %>">
          </div>
          <div class="form-group">
            <label for="name">帐号</label>
            <input name="name" type="text" class="form-control" id="name" placeholder="导航名称" value="<%= nav.name %>">
          </div>
          <div class="form-group">
            <label for="desc">用户名</label>
            <input name="desc"  type="text" class="form-control" id="desc" placeholder="说明" value="<%= nav.desc %>">
          </div>
          <div class="form-group">
            <label for="tag">部门</label>
            <input name="tag" type="text" class="form-control" id="tag" placeholder="TAG" value="<%= nav.tag %>">
          </div>
          <div class="form-group">
            <label for="desc">邮箱</label>
            <input name="desc"  type="text" class="form-control" id="desc" placeholder="说明" value="<%= nav.desc %>">
          </div>
          <div class="form-group">
            <label for="tag">最后登录</label>
            <input name="tag" type="text" class="form-control" id="tag" placeholder="TAG" value="<%= nav.tag %>">
          </div>
          <button type="submit" class="btn btn-blue">提交</button>
          <a href="/navigation" class="btn">返回</a>
        </div>
      </div>
    </div>
  </div>
</div>
